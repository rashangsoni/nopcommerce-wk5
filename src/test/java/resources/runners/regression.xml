<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Regression Suite" verbose="1">
    <groups>
        <run>
            <include name="regression"></include>
        </run>
    </groups>
    <listeners>
        <listener class-name="com.nopcommerce.demo.customelisteners.CustomListeners"></listener>
        <listener class-name="org.uncommons.reportng.HTMLReporter"></listener>
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter"></listener>
    </listeners>
    <test name="regression Test">
        <classes>
            <class name="com.nopcommerce.demo.testsuite.LoginTest"></class>
            <class name="com.nopcommerce.demo.testsuite.ElectronicsTest"></class>

        </classes>
    </test>
</suite>